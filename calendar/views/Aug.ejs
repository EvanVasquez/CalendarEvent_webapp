                  <% for(var i = 0; i < Results.Re.length; i++){ %>
                      <div class = "col-md-6 col-sm-6">
                          <p class="list-group-item fixme"><strong>Day:</strong> <%= Results.Re[i].day %></p>
                          <p class="list-group-item fixme"><strong>Title:</strong> <%= Results.Re[i].title %></p>
                          <p class="list-group-item fixme"><strong>Description:</strong> <%= Results.Re[i].description %></p>
                          <p class="list-group-item fixme"><strong>Time:</strong> <%= Results.Re[i].startTime %> - <%= Results.Re[i].endTime %> </p>
                          <form class="" action="/removeEvent" method="post">
                              <p class="list-group-item fixme"><button name="RemoveEvent" value="<%= Results.Re[i].id %>">Remove Event</button></p>
                          </form>
                          <form class="" action="/UpdateP" method="get">
                              <p class="list-group-item fixme"><button name="Up" value="<%= Results.Re[i].id %>">Update Event</button></p>
                          </form>
